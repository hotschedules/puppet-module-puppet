# vim: filetype=config tabstop=2 expandtab shiftwidth=2 softtabstop=2 foldmethod=marker smartindent
#
# Puppet Windows Agent Configuration
#
[main]

  pluginsync  = true
  autoflush   = true

  environment = <%= @env %>

[agent]
  # disable client RESTful API
  listen      = false

  certname    = <%= @fqdn %>
  server      = <%= @master %>

